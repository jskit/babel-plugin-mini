{
  "name": "babel-plugin-mini",
  "version": "0.0.2",
  "description": "转换小程序",
  "main": "lib/index.js",
  "options": {
    "mini": "--harmony --require babel-core/register test/*.spec.js",
    "mocha": "--harmony --require babel-core/register test/**/*.spec.js"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "benchmark": "npm test && node benchmark",
    "build": "rimraf lib && babel src -d lib --ignore test",
    "prepublish": "npm run build",
    "test2": "mocha --harmony --require babel-core/register test/**/*.spec.js",
    "test": "./node_modules/.bin/_mocha $npm_package_options_mocha",
    "mini": "./node_modules/.bin/_mocha $npm_package_options_mini",
    "lint": "./node_modules/.bin/eslint --ext .js src app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jskit/babel-plugin-mini.git"
  },
  "keywords": [
    "babel-plugin-mini",
    "2mini",
    "aliapp",
    "wxapp",
    "jskit"
  ],
  "author": "cloudyan <1395093509@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jskit/babel-plugin-mini/issues"
  },
  "homepage": "https://github.com/jskit/babel-plugin-mini#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  },
  "devDependencies": {
    "assets": "^3.0.0",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "mocha": "^5.0.0",
    "opn": "^5.2.0",
    "rimraf": "^2.6.2"
  }
}
